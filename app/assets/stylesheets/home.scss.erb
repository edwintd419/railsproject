header {
 background-color: #006600;
 height: 100px;
 border: 10px;
 color: #FFF;

 h1 {
 text-align: center;
 padding-top: 40px;
 }
}
hgroup{
  display: inline-block;
  text-align: center;
  position: relative;
  top: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
  color: #fff;
  border: 5px solid #fff;
  padding: .5em 3em;
  background-color: rgba(0,0,0,.2);
  z-index: 2;
}

hometext {
  position: absolute; 
  top: 200px; 
  left: 0px;
  right: 0px; 
}


footer-copyright {
  font-weight: 300;
  overflow: hidden;
  height: 50px;
  line-height: 50px;
  color: rgba(255, 255, 255, 0.8);
  background-color: rgba(51, 51, 51, 0.08);
}

